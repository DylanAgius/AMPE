// Input file
model_type = "KWCcomplex"

end_time            = 10.
max_timesteps       = 5000

Visit {
   interval = 0.02
   interval_type = "time"
}

TimerManager {
    timer_list = "solv::*::*","xfer::*::*","PFModel::*",
                 "QuatIntegrator::*","EllipticFACOps::*"
    print_threshold = 0.0
}

ModelParameters {
   H_parameter = -1.
   
   Temperature{
      meltingT = 1660.
      temperature = 1659.
      latent_heat = 1.0852e4 // [J/mol*K]
   }
   
   epsilon_phi = 0.685   // [(pJ/um)^1/2]

   phi_mobility     = 21.94
   
   phi_well_scale = 2.4
   phi_well_func_type = "double"

   phi_interp_func_type ="pbg"

   FreeEnergyModel{   
      type = "linear"
   }

   molar_volume = 6.59e-6 // [m^3/mol]

   BoundaryConditions {
       Phase{
         boundary_0 = "slope", "0"
         boundary_1 = "slope", "0"
         boundary_2 = "slope", "0"
         boundary_3 = "slope", "0"
      }
   }
}

ScalarDiagnostics {
   interval = 50.
   interval_type = "time"
}   

Integrator {
   atol = 3.e-5
   Preconditioner{}
}

InitialConditions {
   filename = "test.nc"
}

Geometry{
   periodic_dimension = 0, 0
   coarsest_level_resolution = 32, 256
   x_lo         =  0.,    0. 
   x_up         =  2.5,  20. 
}
